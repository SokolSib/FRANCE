<Window x:Class="TicketWindow.Winows.OtherWindows.Keyboard.WKeyBoardNumPad"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:keyboard="clr-namespace:TicketWindow.Winows.OtherWindows.Keyboard"
        Height="768" Width="1024"
        Title="W_KeyBoard_NumPad" >
    <Grid >
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TextBox HorizontalAlignment="Left"  Grid.ColumnSpan="2" Grid.Row="0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="1014" Height="349"/>

        <StackPanel Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" Margin="0,0,285,0" >
            <keyboard:WKeyboard />
        </StackPanel>

        <StackPanel Grid.Row="1" Grid.Column="1" Margin="222,0,0,0" >
            <keyboard:WNumPadComplet />
        </StackPanel>





    </Grid>
</Window>
