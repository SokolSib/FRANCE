<Application x:Class="ticketwindow.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml" Exit="Application_Exit">
   <Application.Resources>        
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
               <ResourceDictionary Source="Themes/Style.xaml"/>

                         
                <ResourceDictionary Source="Themes/Core.xaml"/>
                <ResourceDictionary Source="Themes/Icons.xaml"/>
                <ResourceDictionary Source="Themes/Brushes.xaml"/>                
                <ResourceDictionary Source="Themes/MyIcons.xaml"/>                
                
              
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
